@using Radzen
@using Radzen.Blazor

<RadzenButton Icon="cancel" Text="Cancel" Variant="Variant.Outlined" Click="@(() => NavigationManager.NavigateTo(Href))"></RadzenButton>

@code {
    [Inject]
    public NavigationManager NavigationManager { get; set; } = null!;

    [Parameter] 
    [EditorRequired] 
    public string Href { get; set; } = null!;
}