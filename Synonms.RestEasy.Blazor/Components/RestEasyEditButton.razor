@using Radzen.Blazor

<RadzenButton Icon="edit" Text="Edit" Click="@(() => NavigationManager.NavigateTo(Href))"></RadzenButton>

@code {
    [Inject]
    public NavigationManager NavigationManager { get; set; } = null!;

    [Parameter] 
    [EditorRequired] 
    public string Href { get; set; } = null!;
}